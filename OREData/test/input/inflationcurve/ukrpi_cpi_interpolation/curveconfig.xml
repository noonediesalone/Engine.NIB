            <Quote>IR_SWAP/RATE/GBP/2D/1D/7Y</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/8Y</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/9Y</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/10Y</Quote>
          </Quotes>
          <Conventions>GBP-OIS</Conventions>
        </Simple>
      </Segments>
      <InterpolationVariable>Discount</InterpolationVariable>
      <InterpolationMethod>LogLinear</InterpolationMethod>
      <YieldCurveDayCounter>A365</YieldCurveDayCounter>
      <Tolerance>0.000000000001</Tolerance>
    </YieldCurve>
  </YieldCurves>
  <InflationCurves>
    <InflationCurve>
      <CurveId>UKRPI_ZC_Swaps</CurveId>
