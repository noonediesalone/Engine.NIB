      <DiscountCurve>GBP-SONIA</DiscountCurve>
      <Segments>
        <Simple>
          <Type>Deposit</Type>
          <Quotes>
            <Quote>MM/RATE/GBP/0D/1D</Quote>
          </Quotes>
          <Conventions>GBP-ON-DEPOSIT</Conventions>
        </Simple>
        <Simple>
          <Type>OIS</Type>
          <Quotes>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/1W</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/1M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/2M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/3M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/4M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/5M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/6M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/9M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/1Y</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/18M</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/2Y</Quote>
            <Quote>IR_SWAP/RATE/GBP/2D/1D/3Y</Quote>
